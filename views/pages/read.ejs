<h1 class="display-4 step">Blog Posts</h1>
    <% posts.forEach(function(post) { %>
        <h1 class="sprite one display-6 ml-4" ><%= post.post %></h1>
        <!-- class="sprite one"></div> -->
        <div class="rounded bg-primary p-3" style="height: 200px;" ><%= post.text %></div>
        <hr>
    <% }); %>